<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="akka" type="Akka.Configuration.Hocon.AkkaConfigurationSection, Akka" />
	</configSections>

	<akka>
		<hocon>
		<![CDATA[						
					akka {
						actor {
										 deployment {
												/videoRepo {
													 router = round-robin-pool
													 nr-of-instances = 2
												}
												
												/watchedVideo {
													 router = round-robin-pool
													 nr-of-instances = 5
												}
									}
							 }
			]]>
		 </hocon>
	</akka>

	<startup> 
				<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
	 </startup>
</configuration>